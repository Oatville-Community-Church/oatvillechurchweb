{
  "name": "oatville-community-church",
  "version": "1.0.0",
  "description": "Modern static website for Oatville Community Church with world-class build process",
  "main": "index.js",
  "scripts": {
    "clean": "node scripts/clean.js",
    "build": "node scripts/build.js",
    "dev": "node scripts/dev.js",
    "serve": "node scripts/serve.js",
    "lint": "node scripts/lint.js",
    "test": "npm run lint",
    "prebuild": "npm run clean",
    "postbuild": "npm run lint",
    "start": "npm run build && npm run serve",
    "build:production": "NODE_ENV=production npm run build && node scripts/optimize.js",
    "optimize": "node scripts/optimize.js",
    "perf": "node scripts/performance.js",
    "deploy": "npm run build:production && npm run serve",
    "analyze": "npm run perf && npm run lint"
  },
  "keywords": [
    "static-site",
    "church-website",
    "tailwindcss",
    "sass",
    "modern-build"
  ],
  "author": "Oatville Community Church",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sharesmallbiz-support/oatvillechurch.git"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@types/node": "^24.2.0",
    "express": "^4.21.2",
    "node-fetch": "^3.3.2",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "chokidar": "^4.0.3",
    "compression": "^1.8.1",
    "sass": "^1.90.0"
  }
}
